{
  "extends": [
    "oclif",
    "oclif-typescript",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-extraneous-import": "off",
    "node/no-extraneous-import": "off",
    "node/no-missing-import": "off",
    "unicorn/no-static-only-class": "off",
    "unicorn/prefer-regexp-test": "off"
  },
  "settings": {
    "no-extraneous-import": "off",
    "import/resolver": {
      "typescript": {}
    }
  }
}
